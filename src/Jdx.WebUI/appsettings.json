{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jdx": {
    "HttpServer": {
      "Enabled": true,
      "Protocol": "HTTP",
      "Port": 8080,
      "BindAddress": "127.0.0.1",
      "UseResolve": false,
      "TimeOut": 3,
      "MaxConnections": 100,
      "DocumentRoot": "/tmp/jumbodogx-www",
      "WelcomeFileName": "index.html",
      "UseHidden": false,
      "UseDot": false,
      "UseDirectoryEnum": false,
      "UseExpansion": false,
      "ServerHeader": "BlackJumboDog Version $v",
      "UseEtag": false,
      "ServerAdmin": "",
      "UseCgi": true,
      "CgiCommands": [
        {
          "Extension": "cgi",
          "Program": "/usr/bin/perl"
        },
        {
          "Extension": "pl",
          "Program": "/usr/bin/perl"
        }
      ],
      "CgiTimeout": 30,
      "CgiPaths": [
        {
          "Path": "/cgi-bin",
          "Directory": "/usr/local/cgi-bin"
        }
      ],
      "UseSsi": false,
      "SsiExt": "html,htm,shtml",
      "UseExec": false,
      "UseWebDav": false,
      "WebDavPaths": [
        {
          "Path": "/webdav",
          "AllowWrite": false,
          "Directory": "/var/webdav"
        }
      ],
      "Aliases": [
        {
          "Name": "/docs",
          "Directory": "/usr/share/doc"
        },
        {
          "Name": "/images",
          "Directory": "/var/www/images"
        }
      ],
      "MimeTypes": [
        {
          "Extension": "txt",
          "MimeType": "text/plain"
        },
        {
          "Extension": "html",
          "MimeType": "text/html"
        },
        {
          "Extension": "css",
          "MimeType": "text/css"
        },
        {
          "Extension": "js",
          "MimeType": "text/javascript"
        },
        {
          "Extension": "json",
          "MimeType": "application/json"
        },
        {
          "Extension": "png",
          "MimeType": "image/png"
        },
        {
          "Extension": "jpg",
          "MimeType": "image/jpeg"
        },
        {
          "Extension": "pdf",
          "MimeType": "application/pdf"
        }
      ],
      "AuthList": [
        {
          "Directory": "/private",
          "AuthName": "Private Area",
          "Require": "user admin"
        }
      ],
      "UserList": [
        {
          "UserName": "admin",
          "Password": "REPLACE_WITH_SECURE_SHA256_HASH"
        }
      ],
      "GroupList": [
        {
          "GroupName": "admins",
          "UserName": "admin"
        }
      ],
      "Encode": "UTF-8",
      "IndexDocument": "\u003Chtml\u003E\u003Cbody\u003E\u003Ch1\u003EDirectory: $URI\u003C/h1\u003E\u003Cul\u003E$LIST\u003C/ul\u003E\u003C/body\u003E\u003C/html\u003E",
      "ErrorDocument": "\u003Chtml\u003E\u003Cbody\u003E\u003Ch1\u003EError $CODE\u003C/h1\u003E\u003Cp\u003E$MSG\u003C/p\u003E\u003C/body\u003E\u003C/html\u003E",
      "UseAutoAcl": true,
      "AutoAclApacheKiller": true,
      "EnableAcl": 0,
      "AclList": [
        {
          "Name": "LocalNetwork",
          "Address": "192.168.1.0/24"
        }
      ],
      "UseKeepAlive": true,
      "KeepAliveTimeout": 5,
      "MaxKeepAliveRequests": 100,
      "UseRangeRequests": true,
      "MaxRangeCount": 20,
      "CertificateFile": "",
      "CertificatePassword": "",
      "VirtualHosts": []
    },
    "FtpServer": {
      "Enabled": false,
      "BindAddress": "0.0.0.0",
      "Port": 2121,
      "TimeOut": 300,
      "MaxConnections": 10,
      "BannerMessage": "BlackJumboDog FTP Server Ready",
      "UseSyst": false,
      "ReservationTime": 30,
      "UseFtps": false,
      "CertificateFile": "",
      "CertificatePassword": "",
      "UserList": [
        {
          "UserName": "anonymous",
          "Password": "",
          "HomeDirectory": "/tmp/jumbodogx-ftp",
          "AccessControl": 1
        },
        {
          "UserName": "admin",
          "Password": "REPLACE_WITH_ENCRYPTED_PASSWORD",
          "HomeDirectory": "/tmp/jumbodogx-ftp/admin",
          "AccessControl": 0
        }
      ],
      "MountList": [],
      "EnableAcl": 0,
      "AclList": [
        {
          "Name": "LocalNetwork",
          "Address": "192.168.1.0/24"
        }
      ]
    },
    "DnsServer": {
      "Enabled": true,
      "Port": 5300
    },
    "Logging": {
      "LogLevel": "Information",
      "MaxEntries": 1000
    }
  }
}